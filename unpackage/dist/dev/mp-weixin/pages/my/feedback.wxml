<view class="feedback-box center column"><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" class="radio-group" bindchange="__e"><label class="label"><radio style="transform:scale(0.7);" value="1" checked="{{true}}" color="#007aff"></radio>内容意见</label><label class="label"><radio style="transform:scale(0.7);" value="2" color="#007aff"></radio>产品建议</label><label class="label"><radio style="transform:scale(0.7);" value="3" color="#007aff"></radio>其他</label></radio-group><textarea maxlength="500" placeholder="请填写具体内容帮助我们了解您的意见和建议" data-event-opts="{{[['input',[['__set_model',['$0','content','$event',[]],['formData']]]]]}}" value="{{formData.content}}" bindinput="__e"></textarea><input maxlength="15" type="text" placeholder="填写您的QQ" data-event-opts="{{[['input',[['__set_model',['$0','qq','$event',[]],['formData']]]]]}}" value="{{formData.qq}}" bindinput="__e"/><input maxlength="25" type="text" placeholder="填写您的微信" data-event-opts="{{[['input',[['__set_model',['$0','weixin','$event',[]],['formData']]]]]}}" value="{{formData.weixin}}" bindinput="__e"/><button loading="{{loading}}" disabled="{{loading}}" type="primary" data-event-opts="{{[['tap',[['submitHandler',['$event']]]]]}}" bindtap="__e">提交</button></view>